+					week_now = week_number(now, 'M')
+					then = v[0]
+					week_then = week_number(then, 'M')
+					diff_days = (now - then).days - ((week_now - week_then) * 2)
+					if diff_days >= flags[k]:
+						v[1] = diff_days - flags[k]
+				else:
+					if 'Skipped' not in k and res['Skipped' + k] is not None:
